					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Response status code is 200\", function () {",
									"    pm.expect(pm.response.code).to.eql(200);",
									"});",
									"",
									"",
									"pm.test(\"Response time is less than 200ms\", function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(200);",
									"});",
									"",
									"",
									"pm.test(\"Response is an array with at least one element\", function () {",
									"    const responseData = pm.response.json();",
									"    ",
									"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
									"});",
									"",
									"",
									"pm.test(\"Response is an array and contains required fields\", function () {",
									"    const responseData = pm.response.json();",
									"    ",
									"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
									"    ",
									"    responseData.forEach(function(item) {",
									"        pm.expect(item).to.be.an('object');",
									"        pm.expect(item).to.have.all.keys('letter', 'url');",
									"        pm.expect(item.letter).to.be.a('string');",
									"        pm.expect(item.url).to.be.a('string');",
									"    });",
									"});",
									"",
									"",
									"pm.test(\"URL is in a valid format\", function () {",
									"    const responseData = pm.response.json();",
									"    ",
									"    pm.expect(responseData).to.be.an('array');",
									"    responseData.forEach(function(author) {",
									"        pm.expect(author).to.have.property('url').that.is.a('string').and.satisfies(url => {",
									"            const urlPattern = /^(ftp|http|https):\\/\\/[^ \"]+$/;",
									"            return urlPattern.test(url);",
									"        }, \"URL format is invalid\");",
									"    });",
									"});",
									"var responseData = pm.response.json();",
									"",
									"// Test the response body structure",
									"pm.test(\"Response body contains expected data structure\", function () {",
									"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
									"    ",
									"    responseData.forEach(function(item) {",
									"        pm.expect(item).to.be.an('object').that.has.all.keys('letter', 'url');",
									"        pm.expect(item.letter).to.be.a('string');",
									"        pm.expect(item.url).to.be.a('string').and.satisfies(url => {",
									"            const urlPattern = /^(ftp|http|https):\\/\\/[^ \"]+$/;",
									"            return urlPattern.test(url);",
									"        }, \"URL format is invalid\");",
									"    });",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
